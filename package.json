{
  "name": "next-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/LiKang6688/next-starter.git"
  },
  "author": "Li Kang <kkllpaul7766@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --no-cache",
    "format:check": "prettier --check . --ignore-unknown",
    "format:write": "prettier --write . --ignore-unknown",
    "types:check": "tsc",
    "validate": "pnpm format:check & pnpm lint & pnpm tsc:check & pnpm test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "typescript": "4.9.5",
    "@next/font": "13.1.6",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "3.2.2",
    "prettier-plugin-packagejson": "2.4.3",
    "msw": "1.0.1"
  },
  "engines": {
    "node": ">=18.11.0",
    "pnpm": ">=7.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
